{
  "name": "Way too bright",
  "description": "A simple dark mode activator",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/img/sun-logo-16.png",
      "32": "/img/sun-logo-32.png",
      "48": "/img/sun-logo-48.png",
      "128": "/img/sun-logo-128.png"
    }
  },
  "icons": {
    "16": "/img/sun-logo-16.png",
    "32": "/img/sun-logo-32.png",
    "48": "/img/sun-logo-48.png",
    "128": "/img/sun-logo-128.png"
  }
}